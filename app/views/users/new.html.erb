<h1>Create an Account</h1>

<%= render partial: "layouts/error", locals: {object: @user} %>

<%= form_for(@user) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>
    
    <%= f.submit "Create Account" %>
<% end %>